{
  "name": "httpbook-monacorenderer",
  "displayName": "Monaco Editor Renderer",
  "description": "Monaco Editor with Find enabled",
  "version": "2.1.1",
  "publisher": "anweber",
  "homepage": "https://github.com/AnWeber/httpbook-monacorenderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnWeber/httpbook-monacorenderer"
  },
  "bugs": {
    "url": "https://github.com/AnWeber/httpbook-monacorenderer/issues"
  },
  "qna": "https://github.com/AnWeber/httpbook-monacorenderer/discussions",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Notebooks"
  ],
  "keywords": [
    "Notebook Renderer",
    "MonacoEditor"
  ],
  "icon": "icon.png",
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "notebookRenderer": [
      {
        "id": "monacorenderer",
        "entrypoint": "./dist/monacoRenderer.js",
        "displayName": "Monaco Renderer",
        "mimeTypes": [
          "application/javascript",
          "application/json",
          "text/x-json",
          "application/x-javascript",
          "application/xml",
          "text/css",
          "text/javascript",
          "text/plain",
          "text/xml",
          "text/latex",
          "text/x-css",
          "text/x-html",
          "text/x-json",
          "text/x-typescript",
          "text/x-abap",
          "text/x-apex",
          "text/x-azcli",
          "text/x-bat",
          "text/x-cameligo",
          "text/x-clojure",
          "text/x-coffee",
          "text/x-cpp",
          "text/x-csharp",
          "text/x-csp",
          "text/x-css",
          "text/x-dart",
          "text/x-dockerfile",
          "text/x-ecl",
          "text/x-fsharp",
          "text/x-go",
          "text/x-graphql",
          "text/x-handlebars",
          "text/x-hcl",
          "text/x-html",
          "text/x-ini",
          "text/x-java",
          "text/x-javascript",
          "text/x-julia",
          "text/x-kotlin",
          "text/x-less",
          "text/x-lexon",
          "text/x-lua",
          "text/x-m3",
          "text/x-mips",
          "text/x-msdax",
          "text/x-mysql",
          "text/x-objective-c/objective",
          "text/x-pascal",
          "text/x-pascaligo",
          "text/x-perl",
          "text/x-pgsql",
          "text/x-php",
          "text/x-postiats",
          "text/x-powerquery",
          "text/x-powershell",
          "text/x-pug",
          "text/x-python",
          "text/x-r",
          "text/x-razor",
          "text/x-redis",
          "text/x-redshift",
          "text/x-restructuredtext",
          "text/x-ruby",
          "text/x-rust",
          "text/x-sb",
          "text/x-scala",
          "text/x-scheme",
          "text/x-scss",
          "text/x-shell",
          "text/x-solidity",
          "text/x-sophia",
          "text/x-sql",
          "text/x-st",
          "text/x-swift",
          "text/x-systemverilog",
          "text/x-tcl",
          "text/x-twig",
          "text/x-typescript",
          "text/x-vb",
          "text/x-xml",
          "text/x-yaml"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "format": "prettier --write --parser typescript \"src/**/*.ts?(x)\"",
    "build": "webpack --mode production --config ./webpack.config.js",
    "start": "webpack --mode development --watch --config ./webpack.config.js",
    "lockfile-lint": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm --validate-checksum --validate-integrity",
    "eslint": "eslint src --ext tsx",
    "lint": "npm run format && npm run eslint && npm run lockfile-lint",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/vscode": "1.73.0",
    "@types/vscode-notebook-renderer": "^1.72.2",
    "@types/webpack-env": "^1.18.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "lockfile-lint": "^4.12.1",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "thread-loader": "^4.0.2",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "monaco-editor": "^0.44.0",
    "preact": "^10.18.1"
  }
}
