{
  "name": "httpbook-monacorenderer",
  "displayName": "httpBook - Monaco Editor Renderer",
  "description": "Monaco Editor for httpbook",
  "version": "1.1.0",
  "publisher": "anweber",
  "homepage": "https://github.com/AnWeber/httpbook-monacorenderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnWeber/httpbook-monacorenderer"
  },
  "bugs": {
    "url": "https://github.com/AnWeber/httpbook-monacorenderer/issues"
  },
  "qna": "https://github.com/AnWeber/httpbook-monacorenderer/discussions",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Notebooks"
  ],
  "keywords": [
    "HTTP",
    "Httpbook",
    "MonacoEditor"
  ],
  "icon": "icon.png",
  "preview": true,
  "activationEvents": [
    "onNotebook:http",
    "onLanguage:http"
  ],
  "main": "./dist/extension.js",
  "capabilities": {
    "virtualWorkspaces": true
  },
  "contributes": {
    "notebookRenderer": [
      {
        "id": "httpbook-monaco",
        "entrypoint": "./dist/monacoRenderer.js",
        "displayName": "httpbook",
        "mimeTypes": [
          "x-application/httpbook-monaco-editor",
          "x-httpbookmonaco/json",
          "x-httpbookmonaco/css",
          "x-httpbookmonaco/html",
          "x-httpbookmonaco/markdown",
          "x-httpbookmonaco/plain",
          "x-httpbookmonaco/javascript",
          "x-httpbookmonaco/xml"
        ]
      }
    ]
  },
  "extensionDependencies": [
    "anweber.httpbook"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production --config ./webpack.config.js",
    "watch": "webpack --mode development --watch --config ./webpack.config.js",
    "lockfile-lint": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm --validate-checksum --validate-integrity",
    "eslint": "eslint src --ext ts",
    "lint": "npm run eslint && npm run lockfile-lint",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "@types/node": "^16.10.9",
    "@types/react": "^17.0.29",
    "@types/vscode": "1.60.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "css-loader": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-plugin-node": "^11.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.4",
		"httpyac": "^4.2.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "lockfile-lint": "^4.6.2",
    "monaco-editor-webpack-plugin": "^5.0.0",
    "style-loader": "^3.3.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0"
  },
  "dependencies": {
    "monaco-editor": "^0.29.1",
    "preact": "^10.5.15"
  }
}
